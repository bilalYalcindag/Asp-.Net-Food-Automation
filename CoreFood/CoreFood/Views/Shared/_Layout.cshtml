<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - CoreFood</title>

    <!-- Bootstrap 5 CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" />

    <!-- Site CSS -->
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/CoreFood.styles.css" asp-append-version="true" />
</head>
<body>
    <!-- Navbar Start -->
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark py-2">
        <div class="container-fluid">
            <a class="navbar-brand fs-5" href="/">CoreFood</a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarContent"
                    aria-controls="navbarContent" aria-expanded="false" aria-label="Menüyü Aç">
                <span class="navbar-toggler-icon"></span>
            </button>

            <div class="collapse navbar-collapse" id="navbarContent">
                <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                    <li class="nav-item">
                        <a class="nav-link active" aria-current="page" href="/">Ana Sayfa</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/Category/Index">Category</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/Food/Index">Food</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/Chart/Index/">Google Pie Chart</a>
                    </li>
                    <li>
                        <a class="nav-link" href="/Chart/Index2/">Google Column Chart</a>
                    </li>
                    <li>
                        <a class="nav-link" href="/Chart/Index3/">Google Dynamic Chart</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/Contact">Popup</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/Chart/Statics">Statics</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/Contact">Settings</a>
                    <li class="nav-item">
                        <a class="nav-link" href="/Login/Logout/">Log Out</a>
                    </li>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
    <!-- Navbar End -->

    <div class="container mt-4">
        @RenderBody()
    </div>

   
    <!-- Bootstrap 5 JS -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>
</body>
</html>
